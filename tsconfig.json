{
  "compilerOptions": {
    "target": "es2018",
    "module": "CommonJS",
    "lib": [
      "ES2018",
      "DOM"
    ],
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true
  },
  "exclude": []
}
